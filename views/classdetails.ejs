        <script type="text/javascript">
            $('.tabButton').click(function(){  
                   
                   $('.tabButton').each(function(){
                       $(this).css('background','#FFFFFF');
   
                       var idName=$(this).attr('id')
                       $('#tabButton'+idName).css('display','none');
                       $(this).css('box-shadow','none');
                   })
                   var idName=$(this).attr('id')
                   $('#tabButton'+idName).css('display','block');
                   $('#'+idName).css('background','#E2EAFA26');
                   $('#'+idName).css('box-shadow','0px 0px 2px #00000029');
                       
            })

        </script>
        <script>
            function classDetails(id){
                const data ={id:id}
                axios.post("/class-details",data).then((res)=>{
                console.log(res.data)
                var html = res.data
                var parser = new DOMParser();
                var doc = parser.parseFromString(html, 'text/html');
                var ContainerData = doc.querySelector(".col-lg-9").innerHTML
                console.log(ContainerData)
                var container = document.querySelector(".col-lg-9")
                container.innerHTML = ContainerData
                })
                
            }
